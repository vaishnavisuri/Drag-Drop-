    border-radius: 5px;
    color: #4862ee;
    display: none;
    text-transform: uppercase;
    text-align: center;
}

.componet-builder-styles .view:hover .viewDiv,
.componet-builder-styles .duplicate:hover .duplicateDiv,
.componet-builder-styles .delete:hover .deleteDiv,
.componet-builder-styles .edit:hover .editDiv {
    color: #4862ee;
    display: block;
    font-size: 12px;
    padding: 8px;
}

.componet-builder-styles .viewDiv:after,
.componet-builder-styles .duplicateDiv:after,
.componet-builder-styles .deleteDiv:after,
.componet-builder-styles .editDiv:after {
    left: 58%;
    top: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(255, 255, 255, 0);
    border-top-color: #fff;
    border-width: 5px;
    margin-left: -10px;
}

.componet-builder-styles .duplicateDiv:after {
    margin-left: -28px !important;
}

.componet-builder-styles .deleteDiv:after {
    margin-left: -20px !important;
}

